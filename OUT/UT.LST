00004B9C                            ;=======================================;
00004B9C                            ;				  TDW					;
00004B9C                            ;=======================================;
00004B9C                            TDW		macro	x1,x2			;
00004B9C                            		dc.b	(((\x1-\x2)&$ff00)>>8)
00004B9C                            		dc.b	((\x1-\x2)&$ff)
00004B9C                            		endm
00004B9C                            
00004B9C                            ;=======================================;
00004B9C                            ;				  JDW					;
00004B9C                            ;=======================================;
00004B9C                            JDW		macro	addr
00004B9C                            		dc.b	(((\addr-*-1)&$ff00)>>8)
00004B9C                            		dc.b	((\addr-*)&$ff)
00004B9C                            		endm
