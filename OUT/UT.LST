000005DC 6400                       		bcc.s	Kos_RLE	; if not, branch
000005DE                            
000005DE 12D8                       		move.b	(a0)+,(a1)+ ; copy byte as-is
000005E0 60E6                       		bra.s	Kos_Loop
000005E2                            ; ===========================================================================
000005E2                            
000005E2                            Kos_RLE:
000005E2 7600                       		moveq	#0,d3
000005E4 E24D                       		lsr.w	#1,d5	; get next bit
